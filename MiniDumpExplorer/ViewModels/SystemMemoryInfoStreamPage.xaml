                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/> <!-- 110 -->
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="0" Grid.Column="0" Text="Stream Index" />
            <TextBlock Grid.Row="0" Grid.Column="1" Text="{x:Bind Index,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" />

            <TextBlock Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Text="System Memory Information" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="15" FontWeight="Bold" />

            <TextBlock Grid.Row="3" Grid.Column="0" Text="Revision" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="3" Grid.Column="1" Text="{x:Bind Revision,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="4" Grid.Column="0" Text="Flags" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="flags" Grid.Row="4" Grid.Column="1" Text="{x:Bind Flags,Converter={StaticResource HexNumberConverter},ConverterParameter=full,Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <ListBox Grid.Row="5" Grid.Column="1" ItemsSource="{x:Bind FlagsList}" Foreground="{ThemeResource TextFillColorSecondaryBrush}" />

            <TextBlock Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="2" Text="Basic Information" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="13" FontWeight="Bold" Margin="0,10,0,0" />

            <TextBlock Grid.Row="7" Grid.Column="0" Text="Timer Resolution" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="7" Grid.Column="1" Text="{x:Bind BasicInfo.TimerResolution,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="8" Grid.Column="0" Text="Page Size" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="basicInfoPageSize" Grid.Row="8" Grid.Column="1" Text="{x:Bind BasicInfo.PageSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="9" Grid.Column="0" Text="Number Of Physical Pages" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <StackPanel Grid.Row="9" Grid.Column="1" Orientation="Horizontal">
                <TextBlock Text="{x:Bind BasicInfo.NumberOfPhysicalPages,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="basicInfoNumberOfPhysicalPagesSize" Text="{x:Bind BasicInfo.NumberOfPhysicalPagesSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="10" Grid.Column="0" Text="Lowest Physical Page Number" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="10" Grid.Column="1" Text="{x:Bind BasicInfo.LowestPhysicalPageNumber,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="11" Grid.Column="0" Text="Highest Physical Page Number" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="11" Grid.Column="1" Text="{x:Bind BasicInfo.HighestPhysicalPageNumber,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="12" Grid.Column="0" Text="Allocation Granularity" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="basicInfoAllocationGranularity"  Grid.Row="12" Grid.Column="1" Text="{x:Bind BasicInfo.AllocationGranularity,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="13" Grid.Column="0" Text="Minimum User Mode Address" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="basicInfoMinimumUserModeAddress" Grid.Row="13" Grid.Column="1" Text="{x:Bind BasicInfo.MinimumUserModeAddress,Converter={StaticResource HexNumberConverter},ConverterParameter=full,Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="14" Grid.Column="0" Text="Maximum User Mode Address" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="basicInfoMaximumUserModeAddress" Grid.Row="14" Grid.Column="1" Text="{x:Bind BasicInfo.MaximumUserModeAddress,Converter={StaticResource HexNumberConverter},ConverterParameter=full,Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="15" Grid.Column="0" Text="Active Processors Affinity Mask" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="basicInfoActiveProcessorsAffinityMask" Grid.Row="15" Grid.Column="1" Text="{x:Bind BasicInfo.ActiveProcessorsAffinityMask,Converter={StaticResource HexNumberConverter},ConverterParameter=full,Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="16" Grid.Column="0" Text="Number Of Processors" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="16" Grid.Column="1" Text="{x:Bind BasicInfo.NumberOfProcessors,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />

            <TextBlock Grid.Row="17" Grid.Column="0" Grid.ColumnSpan="2" Text="File Cache Information" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="13" FontWeight="Bold" Margin="0,10,0,0" />

            <TextBlock Grid.Row="18" Grid.Column="0" Text="Current Size" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="fileCacheInfoCurrentSize"  Grid.Row="18" Grid.Column="1" Text="{x:Bind FileCacheInfo.CurrentSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="19" Grid.Column="0" Text="Peak Size" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="fileCacheInfoPeakSize"  Grid.Row="29" Grid.Column="1" Text="{x:Bind FileCacheInfo.PeakSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="20" Grid.Column="0" Text="Page Fault Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="20" Grid.Column="1" Text="{x:Bind FileCacheInfo.PageFaultCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="21" Grid.Column="0" Text="Minimum Working Set" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="fileCacheInfoMinimumWorkingSet"  Grid.Row="21" Grid.Column="1" Text="{x:Bind FileCacheInfo.MinimumWorkingSet,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="22" Grid.Column="0" Text="Maximum Working Set" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="fileCacheInfoMaximumWorkingSet"  Grid.Row="22" Grid.Column="1" Text="{x:Bind FileCacheInfo.MaximumWorkingSet,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="23" Grid.Column="0" Text="Current Size Including Transition In Pages" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <StackPanel Grid.Row="23" Grid.Column="1" Orientation="Horizontal">
                <TextBlock Text="{x:Bind FileCacheInfo.CurrentSizeIncludingTransitionInPages,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="fileCacheInfoCurrentSizeIncludingTransitionInPagesSize" Text="{x:Bind FileCacheInfo.CurrentSizeIncludingTransitionInPagesSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="24" Grid.Column="0" Text="Peak Size Including Transition In Pages" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <StackPanel Grid.Row="24" Grid.Column="1" Orientation="Horizontal">
                <TextBlock Text="{x:Bind FileCacheInfo.PeakSizeIncludingTransitionInPages,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="fileCacheInfoPeakSizeIncludingTransitionInPagesSize" Text="{x:Bind FileCacheInfo.PeakSizeIncludingTransitionInPagesSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="25" Grid.Column="0" Text="Transition RePurpose Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" Visibility="{x:Bind FileCacheInfo.HasTransitionRePurposeCount,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" />
            <TextBlock Grid.Row="25" Grid.Column="1" Text="{x:Bind FileCacheInfo.TransitionRePurposeCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" Visibility="{x:Bind FileCacheInfo.HasTransitionRePurposeCount,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" />
            <TextBlock Grid.Row="26" Grid.Column="0" Text="Flags" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="fileCacheInfoFlags" Grid.Row="26" Grid.Column="1" Text="{x:Bind FileCacheInfo.Flags,Converter={StaticResource HexNumberConverter},ConverterParameter=full,Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />

            <TextBlock Grid.Row="27" Grid.Column="0" Grid.ColumnSpan="2" Text="Basic Performance Information" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="13" FontWeight="Bold" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" Margin="0,10,0,0" />

            <TextBlock Grid.Row="28" Grid.Column="0" Text="Available Pages" Foreground="{ThemeResource TextFillColorPrimaryBrush}" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" />
            <StackPanel Grid.Row="28" Grid.Column="1" Orientation="Horizontal" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" >
                <TextBlock Text="{x:Bind BasicPerfInfo.AvailablePages,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="basicPerfInfoAvailablePagesSize" Text="{x:Bind BasicPerfInfo.AvailablePagesSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="29" Grid.Column="0" Text="Committed Pages" Foreground="{ThemeResource TextFillColorPrimaryBrush}" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" />
            <StackPanel Grid.Row="29" Grid.Column="1" Orientation="Horizontal" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" >
                <TextBlock Text="{x:Bind BasicPerfInfo.CommittedPages,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="basicPerfInfoCommittedPagesSize" Text="{x:Bind BasicPerfInfo.CommittedPagesSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="30" Grid.Column="0" Text="Commit Limit" Foreground="{ThemeResource TextFillColorPrimaryBrush}" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" />
            <StackPanel Grid.Row="30" Grid.Column="1" Orientation="Horizontal" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" >
                <TextBlock Text="{x:Bind BasicPerfInfo.CommitLimit,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="basicPerfInfoCommitLimitSize" Text="{x:Bind BasicPerfInfo.CommitLimitSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="31" Grid.Column="0" Text="Peak Commitment" Foreground="{ThemeResource TextFillColorPrimaryBrush}" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" />
            <StackPanel Grid.Row="31" Grid.Column="1" Orientation="Horizontal" Visibility="{x:Bind BasicPerfInfo.HasBasicPerfInfo,Converter={StaticResource BoolToVisibilityConverter},Mode=OneWay}" >
                <TextBlock Text="{x:Bind BasicPerfInfo.PeakCommitment,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="basicPerfInfoPeakCommitmentSize" Text="{x:Bind BasicPerfInfo.PeakCommitmentSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>

            <StackPanel Grid.Row="32" Grid.Column="0" Grid.ColumnSpan="2" Orientation="Vertical" Margin="0,10,0,0" >
                <TextBlock Text="Performance Information" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="13" FontWeight="Bold" />
                <TextBlock Text="CC = cache coherence" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="8" Margin="10,0,0,0" />
                <TextBlock Text="MDL = Memory Descriptor List" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="8"  Margin="10,0,0,0" />
                <TextBlock Text="TB = Translation Buffer" Foreground="{ThemeResource TextFillColorPrimaryBrush}" FontSize="8"  Margin="10,0,0,0" />
            </StackPanel>
            
            <TextBlock Grid.Row="33" Grid.Column="0" Text="Idle Process Time" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock x:Name="perfInfoIdleProcessTime"  Grid.Row="33" Grid.Column="1" Text="{x:Bind PerfInfo.IdleProcessTime,Converter={StaticResource DurationConverter},ConverterParameter=filetime,Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="34" Grid.Column="0" Text="IO Read Transfer Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="34" Grid.Column="1" Text="{x:Bind PerfInfo.IoReadTransferCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="35" Grid.Column="0" Text="IO Write Transfer Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="35" Grid.Column="1" Text="{x:Bind PerfInfo.IoWriteTransferCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="36" Grid.Column="0" Text="IO Other Transfer Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="36" Grid.Column="1" Text="{x:Bind PerfInfo.IoOtherTransferCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="37" Grid.Column="0" Text="IO Read Operation Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="37" Grid.Column="1" Text="{x:Bind PerfInfo.IoReadOperationCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="38" Grid.Column="0" Text="IO Write Operation Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="38" Grid.Column="1" Text="{x:Bind PerfInfo.IoWriteOperationCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="39" Grid.Column="0" Text="IO Other Operation Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="39" Grid.Column="1" Text="{x:Bind PerfInfo.IoOtherOperationCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="40" Grid.Column="0" Text="Available Pages" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <StackPanel Grid.Row="40" Grid.Column="1" Orientation="Horizontal" >
                <TextBlock Text="{x:Bind PerfInfo.AvailablePages,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="perfInfoAvailablePagesSize" Text="{x:Bind PerfInfo.AvailablePagesSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="41" Grid.Column="0" Text="Committed Pages" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <StackPanel Grid.Row="41" Grid.Column="1" Orientation="Horizontal" >
                <TextBlock Text="{x:Bind PerfInfo.CommittedPages,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="perfInfoCommittedPagesSize" Text="{x:Bind PerfInfo.CommittedPagesSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="42" Grid.Column="0" Text="Commit Limit" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <StackPanel Grid.Row="42" Grid.Column="1" Orientation="Horizontal" >
                <TextBlock Text="{x:Bind PerfInfo.CommitLimit,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="perfInfoCommitLimitSize" Text="{x:Bind PerfInfo.CommitLimitSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="43" Grid.Column="0" Text="Peak Commitment" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <StackPanel Grid.Row="43" Grid.Column="1" Orientation="Horizontal" >
                <TextBlock Text="{x:Bind PerfInfo.PeakCommitment,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
                <TextBlock Text=" - " Foreground="{ThemeResource TextFillColorPrimaryBrush}" Margin="0,0,5,0" />
                <TextBlock x:Name="perfInfoPeakCommitmentSize" Text="{x:Bind PerfInfo.PeakCommitmentSize,Converter={StaticResource SizeNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            </StackPanel>
            <TextBlock Grid.Row="44" Grid.Column="0" Text="Page Fault Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="44" Grid.Column="1" Text="{x:Bind PerfInfo.PageFaultCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="45" Grid.Column="0" Text="Copy On Write Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="45" Grid.Column="1" Text="{x:Bind PerfInfo.CopyOnWriteCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="46" Grid.Column="0" Text="Transition Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="46" Grid.Column="1" Text="{x:Bind PerfInfo.TransitionCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="47" Grid.Column="0" Text="Cache Transition Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="47" Grid.Column="1" Text="{x:Bind PerfInfo.CacheTransitionCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="48" Grid.Column="0" Text="Demand Zero Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
            <TextBlock Grid.Row="48" Grid.Column="1" Text="{x:Bind PerfInfo.DemandZeroCount,Converter={StaticResource LocalNumberConverter},Mode=OneWay}" Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}" IsTextSelectionEnabled="True" />
            <TextBlock Grid.Row="49" Grid.Column="0" Text="Page Read Count" Foreground="{ThemeResource TextFillColorPrimaryBrush}" />
